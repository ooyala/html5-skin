  },
  "devDependencies": {
    "browserify": "14.5.0",
    "bulk-require": "^1.0.1",
    "bulkify": "^1.4.2",
    "git-rev": "0.2.1",
    "graceful-fs": "*",
    "gulp": "3.9.1",
    "gulp-connect": "^5.0.0",
    "gulp-open": "^2.0.0",
    "gulp-rename": "*",
    "gulp-replace": "0.6.1",
    "gulp-sass": "3.1.0",
    "gulp-shell": "0.6.3",
    "gulp-sourcemaps": "2.6.1",
    "gulp-uglify": "3.0.0",
